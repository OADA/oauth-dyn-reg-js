{
  "name": "oauth-dyn-reg",
  "version": "2.0.2",
  "description": "Implementation of draft OAuth 2.0 Dynamic Client Registration Protocol",
  "main": "dist/index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -b",
    "test": "mocha",
    "test:debug": "mocha --full-trace -t 1000000000",
    "cover": "c8 mocha",
    "clean": "rm -rf ./coverage"
  },
  "mocha": {
    "reporter": "spec",
    "ui": "bdd",
    "timeout": 200
  },
  "ava": {
    "failFast": false,
    "files": [
      "**/*.test.ts"
    ],
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "src/": "dist/",
        "test/": ".test/"
      },
      "compile": false
    }
  },
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true,
    "src": "src",
    "exclude": [
      "*.d.ts",
      ".pnp.*",
      "test",
      ".test"
    ]
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OADA/oauth-dyn-reg-js.git"
  },
  "keywords": [
    "oada",
    "oauth2",
    "draft-ietf-oauth-dyn-reg"
  ],
  "author": {
    "name": "Alex Layton",
    "email": "alex@layton.in",
    "url": "http://alex.layton.in"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OADA/oauth-dyn-reg-js/issues"
  },
  "homepage": "https://github.com/OADA/oauth-dyn-reg-js",
  "devDependencies": {
    "@oada/types": "^1.8.1",
    "@tsconfig/node12": "^1.0.9",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^12.20.47",
    "@types/superagent": "^4.1.15",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@yarnpkg/sdks": "2.6.0",
    "c8": "^7.11.0",
    "chai": "^4.3.6",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.40.0",
    "eslint-config-xo-typescript": "^0.50.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-i18n-text": "^1.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-regexp": "^1.6.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-unicorn": "^41.0.1",
    "mocha": "^9.2.2",
    "nock": "^13.2.4",
    "prettier": "^2.6.1",
    "ts-node": "^10.7.0",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "superagent": "7.1.1",
    "tslib": "^2.3.1"
  },
  "packageManager": "yarn@3.2.0"
}
